<h1>Teams#show</h1>
<p>Find me in app/views/teams/show.html.erb</p>
<h2>Add from following members</h2>

<% @members.each do |m| %>
<%= m.email%><br>
<%end%>

<% form_for @team_mem ,url: member_path(team_id: params[:id],acc_id: params[:account_id]) ,meathod::post do |f| %>
  <table>
    <thead>
      <tr>
        <th>email</th>
      </tr>
    </thead>
    <tbody>
      <div class = "check_box">
        <label class ="checkbox inline">
      <% @members.each do |e| %>
      <tr>
        <td><%= e.email%></td>
        <td><%= check_box_tag "ids[]", e.id %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
  <%= submit_tag "add" %>

<% end %>
<%debugger%>
