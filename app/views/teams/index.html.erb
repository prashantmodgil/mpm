<h1>Teams#index</h1>
<p>Find me in app/views/teams/index.html.erb</p>
<h2>This  account</h2>

<%= form_for :team, url: account_teams_path(acc_id: params[:id]),method: :post do |f| %>
  <div class="form-group">
  <%= f.hidden_field :acc_id,value: params[:account_id] %>
  <%= f.hidden_field :user_id,value: params[:current_user_id] %>

  <%= f.label :team_name %>
  <%= f.text_field :name, autofocus: true, class: "form-control" %>
</div>
  <%= f.submit "create team" %>
</div>
  <%end%>

  <h2>all teams </h2>

  <% @teams.each do |t| %>
    <% if (t.acc_id) == (params[:account_id].to_i) %>

          <%= t.name %>
          <%= link_to 'Show',account_team_path(params[:account_id],t.id) %>
          <br>

    <%end%>
    <%end%>

    <h2>MyTeams</h2>
    <%@my_teams.each do |t|%>
    <ul>
    <%=t.name%>
    </ul>
    <%end%>
<h2>All Teams </h2>
<%@other_teams.each do |t|%>
<ul>
<%= t.name %>
</ul>
  <%end%>
